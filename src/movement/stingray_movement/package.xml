<?xml version="1.0"?>
<package format="2">
	<name>stingray_movement</name>
	<version>0.1.0</version>
	<description>Package contains basic action servers for movement of the vehicle for Stingray platform.</description>

	<maintainer 			email="rentgeny@mail.ru">Timur Akhtyamov				</maintainer>
	<maintainer 			email="vladushked@yandex.ru">Vladislav Plotnikov		</maintainer>


	<license>							MIT											</license>
									
									
	<buildtool_depend>					ament_cmake									</buildtool_depend>
	<buildtool_depend>					rclidl_default_generators					</buildtool_depend>
			
	<depend>							rclcpp										</depend>
	<depend>							rcllib										</depend>
	<depend>							rclpy										</depend>
	<depend>							std_srvs									</depend>
	<depend>							actionlib									</depend>
	<depend>							stingray_utils								</depend>
			
	<depend>							actionlib_msgs								</depend>
	<depend>							stingray_communication_msgs					</depend>
	<depend>							stingray_movement_msgs						</depend>
	<depend>							geometry_msgs								</depend>
	<depend>							std_msgs									</depend>
	<exec_depend>						rclidl_default_runtime						</exec_depend>
	
	<export>
		<build_type>ament_python</build_type>
	  <build_type>ament_cmake</build_type>
	</export>
</package>
